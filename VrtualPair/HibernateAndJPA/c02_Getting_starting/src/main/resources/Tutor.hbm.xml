<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ua.mega.domain.Tutor">
        <id name="id" access="field">
            <generator class="native"/>
        </id>
        <property name="staffId" unique="true" access="field"/>
        <property name="name" access="field"/>
        <property name="salary" access="field" column="RE_SALARY"/>
        <set name="supervisionGroup" access="field" inverse="true">
            <key column="TUTOR_FK"/>
            <one-to-many class="ua.mega.domain.Student"/>
        </set>
        <set name="subjectsQualifiedToTeach" access="field" inverse="true" table="SUBJECT_TUTOR">
            <key column="QUALIFIEDTUTORS_ID"/>
            <many-to-many column="SUBJECT_ID" class="ua.mega.domain.Subject"/>
        </set>
    </class>
</hibernate-mapping>